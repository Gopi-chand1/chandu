@model IEnumerable<MVCApplication.Models.EventModel>

@{
    ViewData["Title"] = "EventLinks";
}

<h1>EventLinks</h1>


<div class="container">
    <p>
        <a asp-action="CreateEvent">Create New</a>
    </p>
    @{
        <ul>
            @foreach (var item in Model)
            {

                <li>@Html.ActionLink(@item.Title, "GetEvent", new { id=@item.EventId })</li>
}
        </ul> }
</div>

